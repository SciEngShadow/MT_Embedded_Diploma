#include<stdio.h>

int main()
{
	int min,max;

	printf("Enter Min:");
	fflush(stdin);fflush(stdout);
	scanf("%d",&min);

	printf("Enter Max:");
	fflush(stdin);fflush(stdout);
	scanf("%d",&max);
}
